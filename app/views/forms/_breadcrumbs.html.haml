- if @category
  #breadcrumbs
    > 
    = link_to @category.name, category_path(@category)
    - if @subcategory
      = " - "
      = link_to @subcategory.name, category_subcategory_path(@category, @subcategory)
      - if @form
        = " - "
        = link_to @form.name, category_subcategory_form_path(@category, @subcategory, @form)
